{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug J-Link",
      "type": "cortex-debug",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build/relax/new_foot.elf",
      "servertype": "jlink",
      "device": "XMC4800-2048",
      "interface": "swd",
      "runToEntryPoint": "main",
      "breakAfterReset": true,
      "svdFile": "${workspaceRoot}/board/XMC/Common/XMC4800.svd",
      "armToolchainPath": "${env:ARM_GCC_BIN_FOLDER}"
      ,"showDevDebugOutput": "raw"
    },
    {
      "name": "Attach J-Link",
      "type": "cortex-debug",
      "request": "attach",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build/new_foot.elf",
      "servertype": "jlink",
      "device": "XMC4800-2048",
      "interface": "swd",
      "svdFile": "${workspaceRoot}/board/XMC/Common/XMC4800.svd",
      "armToolchainPath": "${env:ARM_GCC_BIN_FOLDER}"
    }
  ]
}